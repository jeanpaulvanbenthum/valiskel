  "keywords": [
    "valiskel",
    "laravel",
    "validation",
    "validator"
  ],
  "homepage": "https://github.com/Ilyes512/valiskel",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyes Ahidar",
      "email": "ilyes.ahidar@gmail.com",
      "homepage": "http://ilyes.nl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "~5.0",
    "illuminate/contracts": "~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "Ilyes512\\Valiskel\\": "src"
    }
  },
  "autoload-dev": {
